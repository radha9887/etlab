# ETLab Configuration
# Copy this file to .env and modify as needed

# ============ DATABASE ============
# SQLite (default - embedded, zero-config)
DATABASE_URL=sqlite+aiosqlite:///./data/etlab.db

# PostgreSQL
# DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/etlab

# MySQL
# DATABASE_URL=mysql+aiomysql://user:password@localhost:3306/etlab

# ============ SPARK ============
SPARK_MASTER=local[*]
SPARK_APP_NAME=ETLab

# ============ APPLICATION ============
APP_ENV=production
LOG_LEVEL=INFO
